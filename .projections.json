{
    "piper/*.py": {
        "alternate": "test/test_{}.py",
        "command": "source"
    },
    "test/test_*.py": {
        "alternate": "piper/{}.py",
        "command": "test"
    },
    "piper/cli/*.py": {
        "alternate": "test/cli/test_{}.py",
        "command": "source"
    },
    "test/cli/test_*.py": {
        "alternate": "piper/cli/{}.py",
        "command": "test"
    },
    "piper/db/*.py": {
        "alternate": "test/db/test_{}.py",
        "command": "source"
    },
    "test/db/test_*.py": {
        "alternate": "piper/db/{}.py",
        "command": "test"
    },
}
